{
    "name": "ui",
    "requiredModule" : "'ui.card','ui.content','ui.tab'",
    "config": "config/ui-config.js",
    "modules": [
        {
            "name": "ui.card",
            "service": "card/service/*.js",
            "controller": "card/controller/*.js",
            "directive": "card/directive/*.js",
            "view": "card/view/*.html",
            "requiredModule" : "'teste','teste2'"
        },
        {
            "name": "ui.content",
            "service": "content/service/*.js",
            "controller": "content/controller/*.js",
            "directive": "content/directive/*.js",
            "view": "content/view/*.html",
            "requiredModule" : ""

        },
        {
            "name": "ui.tab",
            "service": "tab/service/*.js",
            "controller": "tab/controller/*.js",
            "directive": "tab/directive/*.js",
            "view": "tab/view/*.html",
            "requiredModule" : ""

        }
    ]
}
